# Copy to deploy/docker/.env.prod and update values before deployment.
MYSQL_ROOT_PASSWORD=change_root_password
DB_NAME=soho_transport
DB_USER=soho_user
DB_PASSWORD=change_db_password

JWT_SECRET=change_access_secret
JWT_EXPIRES_IN=15m
JWT_REFRESH_SECRET=change_refresh_secret
JWT_REFRESH_EXPIRES_IN=7d

# Public origin serving the frontend via Nginx
FRONTEND_ORIGIN=http://localhost
